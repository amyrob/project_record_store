<h2><%= @artist.name %></h2>

<form action="/artists/<%= @artist.id %>/delete" method="post">
<div id="delete_button">
  <button type="submit" >Delete Artist</button>
  </div>
</form>
<div class="container">
<% for album in @artist.albums %>
<img src="../album_art/<%= album.id %>.jpg"/>
<% end %>
</div>
