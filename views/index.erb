<h1>Epic Records...where everything sounds epic!</h1>

<table>
  <tr>
    <th>Album ID:</th>
    <th>Artist:</th>
    <th>Title:</th>
    <th>Buy Price:</th>
    <th>Sell Price:</th>
    <th>Stock Level:</th>

  </tr>

  <% @albums.each do |album| %>

  <tr>
    <td><%= album.id %></td>
    <td><%= album.artist %></td>
    <td><%= album.title %></td>
    <td><%= album.buy_price %></td>
    <td><%= album.sell_price %></td>
    <td><%= album.stock_level %></td>

  </tr>
<% end %>
</table>
